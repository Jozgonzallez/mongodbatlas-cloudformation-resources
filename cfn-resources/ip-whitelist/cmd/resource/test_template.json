{
    "ProjectId": "{{ env `ATLAS_PROJECT_ID` }}",
    "ApiKeys": { 
        "PublicKey": "{{ env `ATLAS_PUBLIC_KEY` }}",
        "PrivateKey": "{{ env `ATLAS_PRIVATE_KEY` }}"
    },
    "Whitelist": [ 
{{ $first := true }}
{{ range $key, $access := .Whitelist }}
    {{ if $first }}
        {{ $first = false }}
    {{ else }}
        ,
    {{ end }}
    { "IpAddress": "{{ default "" $access.IpAddress }}", 
      "CidrBlock": "{{ default "" $access.CidrBlock }}",
      "AwsSecurityGroup": "{{ default "" $access.AwsSecurityGroup }}",
      "xxxx": "{{ now | date_modify `+30d` }}",
      "DeleteAfterDate": "{{ default (now | date_modify `+30d`) $access.DeleteAfterDate }}",
      "Comment": "{{ default "testing" $access.Comment }}" }
{{end}}
    ]
}
