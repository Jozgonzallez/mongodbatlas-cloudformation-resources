{
    "typeName": "MongoDB::Atlas::CloudBackUp",
    "description": "This resource allows to take one on-demand snapshot, get one or all cloud provider snapshot and delete one cloud provider snapshot.",
    "definitions": {
        "apiKeyDefinition": {
            "type": "object",
            "properties": {
                "PublicKey": {
                    "type": "string"
                },
                "PrivateKey": {
                    "type": "string"
                }
            },
            "additionalProperties": false
        }
    },
    "properties": {
        "ProjectId": {
            "description": "The unique identifier of the project for the Atlas cluster.",
            "type": "string"
        },
        "ClusterName": {
            "description": "The name of the Atlas cluster that contains the snapshots you want to retrieve.",
            "type": "string"
        },
        "Id": {
            "description": "Unique identifier of the snapshot.",
            "type": "string"
        },
        "Description": {
            "description": "Description of the on-demand snapshot.",
            "type": "string"
        },
        "ReplicaSetName": {
            "description": "Label given to the replica set from which Atlas took this snapshot. Atlas returns this parameter when \"type\": \"replicaSet\".",
            "type": "string"
        },
        "CloudProvider": {
            "description": "Name of the Cloud Provider",
            "type": "string"
        },
        "RetentionInDays": {
            "description": "The number of days that Atlas should retain the on-demand snapshot. ",
            "type": "string"
        },
        "CreatedAt": {
            "description": "UTC ISO 8601, formatted point in time when Atlas took the snapshot.",
            "type": "string",
            "format": "date-time"
        },
        "ExpiredAt": {
            "description": "UTC ISO 8601, Expiring time for the snapshot.",
            "type": "string",
            "format": "date-time"
        },
        "MasterKeyUuid": {
            "description": "Unique ID of the AWS KMS Customer Master Key used to encrypt the snapshot.",
            "type": "string"
        },
        "CopyRegions": {
            "description": "List that identifies the regions to which MongoDB Cloud copies the snapshot.",
            "items": {
                "type" : "string"
            },
            "type": "array"
        },
        "MongoVersion": {
            "description": "Version of the MongoDB server.",
            "type": "string"
        },
        "SnapshotType": {
            "description": "Specified the type of snapshot.",
            "type": "string"
        },
        "Status": {
            "description": "Current status of the snapshot.",
            "type": "string"
        },
        "Links": {
            "description": "One or more links to sub-resources and/or related resources.",
            "type": "array",
            "items": {
                "type" : "object",
                "properties": {
                    "Rel": {
                        "type": "string"
                    },
                    "Href": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            }
        },
        "StorageSizeBytes": {
            "description": "Specifies the size of the snapshot in bytes.",
            "type": "integer"
        },
        "Type": {
            "description": "Specifies the type of cluster.",
            "type": "string"
        },
        "ApiKeys": {
            "$ref": "#/definitions/apiKeyDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "ProjectId",
        "ClusterName",
        "ApiKeys"
    ],
    "readOnlyProperties": [
        "/properties/Id",
        "/properties/CreatedAt",
        "/properties/ExpiredAt",
        "/properties/CopyRegions",
        "/properties/Links",
        "/properties/ReplicaSetName",
        "/properties/CloudProvider",
        "/properties/MasterKeyUuid",
        "/properties/MongoVersion",
        "/properties/SnapshotType",
        "/properties/Status",
        "/properties/Type",
        "/properties/StorageSizeBytes"
   ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                ""
            ]
        },
        "read": {
            "permissions": [
                ""
            ]
        },
       
        "delete": {
            "permissions": [
                ""
            ]
        },
        "list": {
            "permissions": [
                ""
            ]
        }
    }
}
